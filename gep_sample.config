<xml>
	<Title>This is a sample config file</Title>

	<DataSet>
		<FileType>Datafile_type</FileType>
		<FileLocation>File_location</FileLocation>
		<FileName>file_name</FileName>
		<TrainPercentage>0.7</TrainPercentage>
		<Inputs>6</Inputs>
		<Classes>5</Classes>
	</DataSet>

	<Runs>1</Runs>
	<Generations>100</Generations>
	<MaxRunTime>600</MaxRunTime>
	<PopulationSize>100</PopulationSize>

	<NodeDescription>
		<Head>3</Head>
		<FunctionSet>
			<AddFunction/>
			<SubtractFunction/>
			<MultiplyFunction/>
			<DivideFunction/>
		</FunctionSet>
	</NodeDescription>

	<NodeLayers>
		<Layer>
			<Nodes>3</Nodes>
		</Layer>
		<Layer>
			<Nodes>5</Nodes>
		</Layer>
	</NodeLayers>

	<CellDescription>
		<Head>6</Head>
		<FunctionSet>
			<AddFunction/>
			<SubtractFunction/>
			<MultiplyFunction/>
			<DivideFunction/>
		</FunctionSet>
	</CellDescription>

	<Selection>
		<Method>RouletteKeepBest</Method>
		<PercentageKept>0.50</PercentageKept>
	</Selection>

	<Crossovers>
		<OnePointRecombination probability="1"/>
		<SwapChromosome probability="1"/>
		<CopyParentA probability="1"/>
	</Crossovers>
	
	<MutationRate>0.3</MutationRate>
	<Mutators>
		<RandomReplacement probability="1"/>
		<MakeComplex probability="1"/>
		<HeadInverter probability="1"/>
	</Mutators>

