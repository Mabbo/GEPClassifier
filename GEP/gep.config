<xml>
	<Title>This is a sample config file</Title>

	<DataSet>
		<File>/home/mabbo/datasets/letter-recognition.data</File>
		<DataSetLoader builtin="NumericalDataSetLoader" parameters="ClassIndex=0" />
		<TrainPercentage>0.8</TrainPercentage>
		<NumberOfInputs>17</NumberOfInputs>
		<NumberOfClasses>26</NumberOfClasses>
	</DataSet>

	<Runs>1</Runs>
	<Generations>100</Generations>
	<PopulationSize>250</PopulationSize>

	<NodeDescription>
		<Head>3</Head>
		<FunctionSet>
			<Function builtin="AddFunction" />
			<Function builtin="SubtractFunction" />
			<Function builtin="MultiplyFunction" />
			<Function builtin="DivideFunction" />
			<Function location="/home/mabbo/GEPClassifier/ExternalLibs/bin/"
				  classfile="SineFunction" />
			<Function location="/home/mabbo/GEPClassifier/ExternalLibs/bin/"
				  classfile="CosFunction" />
		</FunctionSet>
		<RNC>10</RNC>
	</NodeDescription>

	<NodeLayers>
		<Layer><Nodes>20</Nodes></Layer>
	</NodeLayers>

	<Fitness builtin="AddOneOnCorrect" />

	<Selection builtin="BasicElitism" parameters="keep=0.7" />

	<Crossovers>
		<Crossover builtin="OnePointPerLayer" weight="3"/>
		<Crossover builtin="CopyParentA" weight="3"/>
	</Crossovers>
	
	<MutationRate>0.5</MutationRate>
	<Mutators>
		<Mutator builtin="RandomReplacement" weight="1" />
		<Mutator builtin="RandomizeGene" weight="1" />
		<Mutator location="/home/mabbo/GEPClassifier/ExternalLibs/bin"
			  classfile="Complexify" weight="2" />
	</Mutators>


	<Processes>
		<StartProcesses>
		</StartProcesses>
		<EndProcesses>
		</EndProcesses>
		<BeforeRunProcesses>
			<Process builtin="OutputConfig" />
		</BeforeRunProcesses>
	
		<BeforeGenerationProcesses>
		</BeforeGenerationProcesses>
		<EndGenerationProcesses>
			<Process builtin="OutputBestScore" />
			<Process classfile="ModulatingMutation" 
				location="/home/mabbo/GEPClassifier/ExternalLibs/bin"/>
		</EndGenerationProcesses>	

		<EndRunProcesses>
			<Process builtin="OutputBestScore" />
		</EndRunProcesses>
	</Processes>


</xml>






