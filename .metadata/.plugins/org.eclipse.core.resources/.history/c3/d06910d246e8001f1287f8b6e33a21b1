package functions;

import GEPClassify.Function;

public class AddFunctions implements Function<Double, Double> {

	@Override
	public Double ApplyFunction(Double[] args) {
		return (args[0] + args[1]);
	}

	@Override
	public int getNumArgs() {
		return 2;
	}

	@Override
	public char getSymbol() {
		return "+";
	}

}
